{
  "extends": [
    "tslint:recommended",
    "tslint-react",
    "tslint-config-prettier"
  ],
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts",
      "./src/setupTests.ts"
    ]
  },
  "rules": {
    "no-empty-interface": false,
    "ordered-imports": false,
    "no-console": false,
    "member-ordering": false,
    "jsx-no-lambda": false,
    "object-literal-sort-keys": false
  }
}